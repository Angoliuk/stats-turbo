{
  "name": "@stats/shared-web",
  "version": "0.0.0",
  "licenses": [
    {}
  ],
  "exports": {
    "./constants": {
      "default": "./dist/constants/index.js",
      "types": "./src/constants/index.ts"
    },
    "./interfaces": {
      "default": "./dist/interfaces/index.js",
      "types": "./src/interfaces/index.ts"
    },
    "./utils": {
      "default": "./dist/utils/index.js",
      "types": "./src/utils/index.ts"
    },
    "./components": {
      "default": "./dist/components/index.js",
      "types": "./src/components/index.ts"
    },
    "./definitions": {
      "types": "./src/definitions.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "build:verify": "tsc --noEmit",
    "lint": "concurrently \"eslint --ext '.tsx,.ts' --max-warnings=0 . \" \"prettier --check './{src,test}/**/*.{ts,tsx}'\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@stats/config": "workspace:*",
    "@stats/eslint-config-custom": "workspace:*",
    "@stats/tailwind": "workspace:*",
    "@stats/tsconfig": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/node": "14.14.14",
    "@types/react": "^18.2.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "resolutions": {
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.11"
  },
  "private": true
}
