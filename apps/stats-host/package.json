{
  "name": "@stats/host",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5521 --strictPort",
    "build": "vite build",
    "docker": "docker-compose -f ../../docker-compose.micro.yaml up",
    "docker:build": "docker-compose -f ../../docker-compose.micro.yaml up --build",
    "preview": "vite preview --port 5521 --strictPort",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "concurrently \"eslint --ext '.tsx,.ts' --max-warnings=0 . \" \"prettier --check './{src,test}/**/*.{ts,tsx}' --config '../../.prettierrc'\" \"tsc --noEmit \"",
    "lint:fix": "eslint . --ext .ts --fix && prettier --write './{src,test}/**/*.ts' "
  },
  "dependencies": {
    "@tanstack/react-router": "0.0.1-beta.185",
    "@trpc/client": "^10.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "1.12.2",
    "@trpc/react-query": "^10.14.1",
    "@tanstack/react-query": "^4.29.7"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.2.1",
    "@stats/eslint-config-custom": "workspace:*",
    "@stats/shared-web": "workspace:*",
    "@stats/tailwind": "workspace:*",
    "@stats/tsconfig": "workspace:*",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.0"
  }
}
